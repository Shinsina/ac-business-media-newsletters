{
  "<common-header-wrapper>": {
    "template": "./wrapper.marko",
    "@separator-position": "string",
    "@separator-type": "string",
    "@clear-floats": "boolean",
    "@date": "object",
    "@date-style": "object"
  },
  "<common-default-header-block>": {
    "template": "./image-full.marko",
    "<wrapper>": {
      "@separator-position": "string",
      "@separator-type": "string",
      "@clear-floats": "boolean",
      "@date": "object",
      "@date-style": "object"
    },
    "@name": "string",
    "@href": "string",
    "@image-src-left": "string",
    "@image-src-right": "string",
    "@image-width-left": "number",
    "@image-width-right": "number",
    "@bg-color": "string"
  },
  "<common-header-view-online-block>": {
    "template": "./view-online.marko"
  },
  "<common-header-image-block>": {
    "template": "./image.marko",
    "@newsletter": "object",
    "@date": "object",
    "@date-style": "object",
    "@name": "string",
    "@href": "string",
    "@image-src": "string",
    "@image-width": "number",
    "@show-date": {
      "type": "boolean",
      "default-value": true
    }
  },
  "<common-psc-header-image-block>": {
    "template": "./psc-image.marko",
    "@newsletter": "object",
    "@name": "string",
    "@image-src-left": "string",
    "@image-src-right": "string",
    "@href": "string"
  },
  "<psc-header-block>": {
    "template": "./psc-block.marko",
    "@date": "object",
    "@image-src": "string",
    "@background-color": "string"
  }
}
